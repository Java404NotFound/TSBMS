<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://www.springframework.org/schema/beans"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.2.xsd ">

	<bean name="BillAction" class="com.rjxy.action.BillAction">
		<property name="BillServiceImpl" ref="BillServiceImpl"></property>
	</bean>
	<bean name="SupplierAction" class="com.rjxy.action.SupplierAction">
		<property name="SupplierServiceImpl" ref="SupplierServiceImpl"></property>
	</bean>
	<bean name="UserAction" class="com.rjxy.action.UserAction">
		<property name="UserServiceImpl" ref="UserServiceImpl"></property>
	</bean>
</beans>